FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 6.416558400000001e-08; // 

boundaryField
{
    Inlet
    {
      type             inletOutlet; 
      inletValue       $internalField;
    }

    Outlet
    {
      type              zeroGradient;
    }

    Top
    {
      type              zeroGradient;
    }

    Side
    {
        type            empty;
    }

    Airfoil
    {
        type            kqRWallFunction;
        value           uniform 0;//$internalField;
    }

    Floor
    {
        type            kqRWallFunction;
        value           uniform 0;//$internalField;
    }
}
// ************************************************************************* //
